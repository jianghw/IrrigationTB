<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".supervisor.fragment.SuMeFragment">

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipe_refresh_visit"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:background="@color/module_segment_line_gray" />

            <RelativeLayout
                android:id="@+id/rl_su_my_qrCode"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fgt_me_rl_height"
                android:background="@drawable/bg_me_control_white">

                <ImageView
                    android:id="@+id/imgView_su_my_head"
                    android:layout_width="@dimen/fgt_me_img_height"
                    android:layout_height="@dimen/fgt_me_img_height"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/divider_line_marginLeft"
                    android:src="@drawable/ic_user_info_head" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/divider_line_marginLeft"
                    android:layout_toRightOf="@+id/imgView_su_my_head"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_me_account"
                        style="@style/MeContentText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        tools:text="xiaoxiaodewo" />

                    <TextView
                        android:id="@+id/tv_su_my_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/next_control_marginTop"
                        android:gravity="center_vertical"
                        android:textColor="@color/activity_hint_gray"
                        android:textSize="?attr/fontSize_1"
                        tools:text="ID:12345678" />
                </LinearLayout>
<!--
                <ImageView
                    android:id="@+id/imgView_su_my_qrCode"
                    android:layout_width="@dimen/fgt_me_qrCode_height"
                    android:layout_height="@dimen/fgt_me_qrCode_height"
                    android:layout_centerVertical="true"
