            <ImageView
                android:layout_width="match_parent"
                android:layout_height="30px"
                android:background="@color/module_segment_line_gray" />

            <RelativeLayout
                android:id="@+id/rl_me_su_my_visit"
                android:layout_width="match_parent"
                android:layout_height="@dimen/activity_layout_height"
                android:background="@drawable/bg_me_control_white">

                <ImageView
                    android:id="@+id/img_su_my_visit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/divider_line_marginLeft"
                    android:src="@drawable/ic_su_visit" />

                <TextView
                    android:id="@+id/tv_su_my_visit"
                    style="@style/MeContentText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/big_btn_marginLeft"
                    android:layout_toRightOf="@+id/img_su_my_visit"
                    android:text="@string/suMain_affirm" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/divider_line_marginLeft"
                    android:src="@drawable/ic_instructions_right" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="30px"
                android:background="@color/module_segment_line_gray" />

            <RelativeLayout
                android:id="@+id/rl_me_su_my_general"
                android:layout_width="match_parent"
                android:layout_height="@dimen/activity_layout_height"
                android:background="@drawable/bg_me_control_white">

                <ImageView
                    android:id="@+id/img_su_my_general"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/divider_line_marginLeft"
                    android:src="@drawable/ic_me_general" />

                <TextView
                    android:id="@+id/tv_su_my_general"
                    style="@style/MeContentText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/big_btn_marginLeft"
                    android:layout_toRightOf="@+id/img_su_my_general"
                    android:text="@string/fgt_me_general" />

                <ImageView
                    android:id="@+id/iv_su_my_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/big_btn_marginLeft"
                    android:layout_toRightOf="@+id/tv_su_my_general"
                    android:src="@drawable/ic_me_new" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/divider_line_marginLeft"
                    android:src="@drawable/ic_instructions_right" />
            </RelativeLayout>


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/module_segment_line_gray" />
        </LinearLayout>
    </android.support.v4.widget.SwipeRefreshLayout>
</FrameLayout>
